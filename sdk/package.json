{
  "name": "@mobi360/sdk",
  "version": "1.0.0",
  "description": "Official TypeScript SDK for Mobility Ops 360 API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "mobi360",
    "mobility",
    "taxi",
    "dispatch",
    "api",
    "sdk",
    "typescript"
  ],
  "author": "Mobility Ops 360",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mobility360/sdk-typescript"
  },
  "dependencies": {
    "axios": "^1.6.5",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}